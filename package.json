{
    "name": "pasmo",
    "displayName": "pasmo",
    "description": "Z80 Assembler",
    "version": "0.0.2",
    "publisher": "boukichi",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pasmo",
                "aliases": [
                    "pasmo"
                ],
                "extensions": [
                    ".asm",
                    ".z80"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pasmo",
                "scopeName": "source.z80",
                "path": "./syntaxes/assembler.tmLanguage.json"
            }
        ]
    },
    "license": "https://github.com/BouKiCHi/vscode-pasmo/blob/master/LICENSE",
    "homepage": "https://github.com/BouKiCHi/vscode-pasmo/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/BouKiCHi/vscode-pasmo.git"
    }
}